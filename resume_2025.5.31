        const rect = cat.getBoundingClientRect();
        const catCenterX = rect.left + rect.width / 2;
        const catCenterY = rect.top + rect.height / 2;
        
        const mouseX = e.clientX;
        const mouseY = e.clientY;
        
        const deltaX = mouseX - catCenterX;
        const deltaY = mouseY - catCenterY;
        
        const distance = Math.sqrt(deltaX * deltaX + deltaY * deltaY);
        const maxDistance = 4; // Maximum pupil movement
        
        const moveX = Math.min(maxDistance, (deltaX / distance) * maxDistance * Math.min(distance / 100, 1));
        const moveY = Math.min(maxDistance, (deltaY / distance) * maxDistance * Math.min(distance / 100, 1));
        
        pupils.forEach(pupil => {
            pupil.style.transform = `translate(calc(-50% + ${moveX}px), calc(-50% + ${moveY}px))`;
        });
    }
    
