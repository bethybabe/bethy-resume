    // Memphis-style popup function
    function showMemphisPopup(message, type = 'info') {
        // Remove existing popup if any
        const existingPopup = document.querySelector('.memphis-popup');
        if (existingPopup) {
            existingPopup.remove();
        }
        
        const popup = document.createElement('div');
        popup.className = 'memphis-popup';
        popup.style.cssText = `
            position: fixed;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%) rotate(-2deg);
            background: ${type === 'success' ? 'var(--memphis-green)' : type === 'error' ? 'var(--memphis-red)' : 'var(--primary-yellow)'};
            border: 4px solid var(--dark-color);
            border-radius: 20px;
            padding: 25px 35px;
            box-shadow: 8px 8px 0 var(--primary-blue);
            z-index: 10000;
            font-family: var(--font-heading);
            font-size: 1.2rem;
            font-weight: bold;
            color: var(--dark-color);
            text-align: center;
            white-space: pre-line;
            max-width: 350px;
            animation: memphisPopupIn 0.5s cubic-bezier(0.68, -0.55, 0.265, 1.55);
        `;
        
        popup.textContent = message;
        
        // Add close button
        const closeBtn = document.createElement('div');
        closeBtn.textContent = '‚úï';
        closeBtn.style.cssText = `
            position: absolute;
            top: -10px;
            right: -10px;
            width: 30px;
            height: 30px;
            background: var(--memphis-red);
            border: 3px solid var(--dark-color);
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            cursor: pointer;
            font-size: 14px;
            font-weight: bold;
            color: white;
        `;
        
        closeBtn.addEventListener('click', () => {
            popup.style.animation = 'memphisPopupOut 0.3s ease forwards';
            setTimeout(() => popup.remove(), 300);
        });
        
        popup.appendChild(closeBtn);
        document.body.appendChild(popup);
        
        // Auto close after 4 seconds
        setTimeout(() => {
            if (popup.parentNode) {
                popup.style.animation = 'memphisPopupOut 0.3s ease forwards';
                setTimeout(() => popup.remove(), 300);
            }
        }, 4000);
    }
    
    // Add Memphis popup animations to CSS
    const memphisPopupStyle = document.createElement('style');
    memphisPopupStyle.textContent = `
        @keyframes memphisPopupIn {
            0% {
                opacity: 0;
                transform: translate(-50%, -50%) rotate(-2deg) scale(0.5);
            }
            100% {
                opacity: 1;
                transform: translate(-50%, -50%) rotate(-2deg) scale(1);
            }
        }
        
        @keyframes memphisPopupOut {
            0% {
