        // ÊâßË°åÊªöÂä®
        scrollToCurrentSectionTop();
        
        // ÁßªÈô§Âä®ÁîªÁ±ª
        setTimeout(() => {
            this.classList.remove('clicked');
        }, 600);
    });
    
    // ÂàùÂßãÂåñÊó∂Ê£ÄÊü•ÊªöÂä®‰ΩçÁΩÆ
