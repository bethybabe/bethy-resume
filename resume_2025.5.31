    // üöÄ ÂõûÂà∞È°∂ÈÉ®ÊåâÈíÆÂäüËÉΩ
    const backToTopBtn = document.getElementById('back-to-top');
    let isScrolling = false;
    
    // ÊªöÂä®Ê£ÄÊµã - ÂçäÂ±èÈ´òÂ∫¶ÂêéÊòæÁ§∫ÊåâÈíÆ
    function handleScroll() {
        if (isScrolling) return;
        
        isScrolling = true;
        requestAnimationFrame(() => {
            const scrollY = window.pageYOffset || document.documentElement.scrollTop;
            const halfScreenHeight = window.innerHeight * 0.5; // Êîπ‰∏∫ÂçäÂ±èÈ´òÂ∫¶
            
            if (scrollY > halfScreenHeight) {
                backToTopBtn.classList.add('show');
            } else {
                backToTopBtn.classList.remove('show');
            }
            
            isScrolling = false;
        });
    }
    
    // ÁõëÂê¨ÊªöÂä®‰∫ã‰ª∂
    window.addEventListener('scroll', handleScroll, { passive: true });
    
    // ÂõûÂà∞ÂΩìÂâçÊ¥ªË∑ÉtabÈ°∂ÈÉ®ÂäüËÉΩ
    function scrollToCurrentSectionTop() {
        // Áõ¥Êé•ÂõûÂà∞È°µÈù¢ÊúÄÈ°∂ÈÉ®
        window.scrollTo({
            top: 0,
            behavior: 'smooth'
        });
    }
    
    // ÁÇπÂáª‰∫ã‰ª∂ - Ê∑ªÂä†ÂõûÂºπÂä®Áîª
    backToTopBtn.addEventListener('click', function() {
        // Ê∑ªÂä†ÁÇπÂáªÂä®Áîª
